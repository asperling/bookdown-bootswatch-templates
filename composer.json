{
    "name": "tobiju/bookdown-bootswatch-templates",
    "type": "library",
    "description": "Bookdown.io With Bootswatch Styles And Prism Syntax Highlighting",
    "keywords": [
        "bookdown",
        "templates",
        "syntax",
        "highlighting",
        "docbook",
        "markdown",
        "manual",
        "documentation",
        "static site"
    ],
    "homepage": "https://github.com/tobiju/bookdown-bootswatch-templates",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Bookdown.Project Contributors",
            "homepage": "https://github.com/bookdown/Bookdown.Project/contributors"
        }
    ],
    "require": {
        "bookdown/bookdown": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "book"
        ]
    }
}
